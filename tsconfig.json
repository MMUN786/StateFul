{
  "compilerOptions": {
    "target": "ES2015",
    "module": "esnext",
    "lib": ["esnext", "DOM"],
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
